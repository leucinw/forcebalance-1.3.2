# ForceBalance input file generated by MakeInputFile.py
# The octothorpe '#' is a comment symbol
# Note: If the specified value is 'None' then the option will truly be set to None - not the string 'None'
# Note: 'Section' option types are more complicated and may require you to read the documentation
# Note: Boolean option types require no value, the key being present implies 'True'

$options
# (string) Directory containing force fields, relative to project directory
ffdir forcefield

# (string) Type of the penalty, L2 or L1 in the optimizer
penalty_type L2

# (allcap) The job type, defaults to a single-point evaluation of objective function
jobtype single

# Path to TINKER
tinkerpath /home/leeping/opt/tinker/bin/

# (list) The names of force fields, corresponding to directory forcefields/file_name.(itp|gen)
forcefield water.prm

# (int) Maximum number of steps in an optimization
maxstep 100

# (float) Convergence criterion of step size (just needs to fall below this threshold)
convergence_step 0.01

# (float) Convergence criterion of objective function (in MainOptimizer this is the stdev of x2 over 10 steps)
convergence_objective 0.1

# (float) Convergence criterion of gradient norm
convergence_gradient 0.1

# (float) Step size for finite difference derivatives in many functions (get_(G/H) in fitsim, FDCheckG)
finite_difference_h 0.001

# (float) Factor for multiplicative penalty function in objective function
penalty_additive 1.0

trust0 0.25
mintrust 0.05
error_tolerance 5.0
adaptive_factor 0.2
adaptive_damping 1.0
normalize_weights no
print_hessian

amoeba_pol mutual
amoeba_eps 1e-5

# Charge constraints are taken care of using "evals".
constrain_charge false

backup false
$end


$target
# (string) The name of the target, which corresponds to the directory targets/dir_name
name Liquid

# (allcap) The type of target, for instance AbInitio_GMXX2
type Liquid_TINKER

# (float) Weight of the current target (with respect to other targets)
weight 1.0

w_rho 1.0
w_hvap 1.0
w_alpha 1.0
w_kappa 1.0
w_cp 1.0
w_eps0 1.0

gas_eq_steps 1000
gas_md_steps 10000
gas_timestep 1.0
gas_interval 0.1

liquid_eq_steps 1000
liquid_md_steps 10000
liquid_timestep 1.0
liquid_interval 0.1

md_threads 4
$end

$target
name cluster-02
# name cluster-02 cluster-03 cluster-04 cluster-05 cluster-06 cluster-07 cluster-08 cluster-09 cluster-10 cluster-11 cluster-12 cluster-13 cluster-14 cluster-15 cluster-16 cluster-17 cluster-18 cluster-19 cluster-20 cluster-21 cluster-22
type AbInitio_TINKER
fitatoms 0
shots -1
w_energy 1.0
w_force 1.0
w_netforce 1.0
w_torque 1.0
weight 0.25
force_map molecule
$end

$target
# Small bugfix in moments.
# (string) The name of the target, which corresponds to the directory targets/dir_name
name Monomer_Moments

# (allcap) The type of target, for instance AbInitio_GMXX2
type Moments_TINKER

# (float) Weight of the current target (with respect to other targets)
weight 1.0

optimize_geometry no

# (float) Wavenumber tolerance, normalizes the objective function.
dipole_denom 0.2
quadrupole_denom 1.0
polarizability_denom 0.2
$end

$target
# (string) The name of the target, which corresponds to the directory targets/dir_name
name DimerMin

# (allcap) The type of target, for instance AbInitio_GMXX2
type BindingEnergy_TINKER

# (float) Weight of the current target (with respect to other targets)
weight 5.0

# (float) The energy denominator in kcal/mol used to normalize the objective function.
energy_denom 1.0
rmsd_denom 0.1

$end

